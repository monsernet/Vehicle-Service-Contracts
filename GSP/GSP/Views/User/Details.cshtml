@model AuthSystem.Areas.Identity.Data.ApplicationUser

@{
    ViewData["Title"] = "User Details";
}

<div class="container">
    <form >
        <div class="mb-3">
            <label class="form-label">Id</label>
            <input type="text" class="form-control"  asp-for="Id" disabled />
        </div>
        <div class="mb-3">
            <label class="form-label">First Name</label>
            <input type="text" class="form-control" asp-for="FirstName" disabled />
        </div>
        <div class="mb-3">
            <label class="form-label">Last Name</label>
            <input type="text" class="form-control" asp-for="LastName" disabled />
        </div>
        <div class="mb-3">
            <label class="form-label">Email</label>
            <input type="text" class="form-control" id="name" asp-for="Email" disabled />
        </div>
        <div class="mb-3">
            <label class="form-label">Username</label>
            <input type="text" class="form-control" id="name" asp-for="UserName" disabled />
        </div>

        
    </form>
</div>

